-a always,exit -F arch=b64 -S execve -C uid!=euid -F euid=0 -k execpriv&#xD;
-a always,exit -F arch=b32 -S execve -C gid!=egid -F egid=0 -k execpriv&#xD;
-a always,exit -F arch=b64 -S execve -C gid!=egid -F egid=0 -k execpriv&#xD;
